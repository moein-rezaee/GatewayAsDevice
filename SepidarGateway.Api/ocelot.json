{
  "Routes": [
    {
      "RouteId": "sample-service",
      "DownstreamPathTemplate": "/api/sample/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6000
        }
      ],
      "UpstreamPathTemplate": "/sample/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete"
      ],
      "SwaggerKey": "sample"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "sample",
      "Config": [
        {
          "Name": "Sample Service",
          "Version": "v1",
          "Url": "http://localhost:6000/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5090",
    "RequestIdKey": "X-Request-Id"
  }
}
